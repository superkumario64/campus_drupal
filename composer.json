{
    "name": "pantheon-upstreams/drupal-project",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/templates",
                "type": "drupal-library",
                "version": "4.11.1",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.11.1.zip",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.10",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^1.9",
        "drupal/admin_toolbar": "^3.0",
        "drupal/allowed_formats": "^1.3",
        "drupal/asset_injector": "^2.7",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/backup_migrate": "^4.1",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/block_class": "^1.3",
        "drupal/captcha": "^1.1",
        "drupal/ckeditor_templates": "^1.2",
        "drupal/color_field": "^2.4",
        "drupal/config_filter": "^1.8",
        "drupal/config_partial_export": "^1.6",
        "drupal/config_split": "^1.7",
        "drupal/console": "^1.0.2",
        "drupal/core-vendor-hardening": "^9.1",
        "drupal/crop": "^2.1",
        "drupal/ctools": "^3.4",
        "drupal/devel": "^4.1",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/devel_php": "^1.2",
        "drupal/dropzonejs": "^2.3",
        "drupal/easy_breadcrumb": "^1.15",
        "drupal/entity": "^1.2",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/fences": "^2.0@RC",
        "drupal/field_group": "^3.1",
        "drupal/file_mdm": "^2.1",
        "drupal/focal_point": "^1.5",
        "drupal/fontawesome": "^2.18",
        "drupal/fontawesome_menu_icons": "^1.9",
        "drupal/google_analytics": "^3.1",
        "drupal/group": "^1.3",
        "drupal/groupmenu": "^1.0@beta",
        "drupal/gtranslate": "^1.14",
        "drupal/html_title": "^1.1",
        "drupal/imagemagick": "^3.1",
        "drupal/jquery_ui_datepicker": "^1.1",
        "drupal/libraries": "^3.0@beta",
        "drupal/link_attributes": "^1.11",
        "drupal/link_class": "^2.0",
        "drupal/linked_field": "^1.3",
        "drupal/linkit": "^6.0@beta",
        "drupal/markup": "^1.0@beta",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_bulk_upload": "^1.0@alpha",
        "drupal/media_entity_file_replace": "^1.0@beta",
        "drupal/menu_block": "1.x-dev",
        "drupal/metatag": "^1.16",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_tools": "^5.0",
        "drupal/module_filter": "^3.1",
        "drupal/name": "1.x-dev",
        "drupal/node_auto_expire": "^1",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/quick_node_clone": "^1.14",
        "drupal/quicklink": "^1.3",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.6",
        "drupal/search_api": "^1.17",
        "drupal/search_api_solr": "^4.1",
        "drupal/simplesamlphp_auth": "^3.2",
        "drupal/smtp": "^1.0",
        "drupal/sophron": "^1.1",
        "drupal/soundcloudfield": "^1.0@alpha",
        "drupal/switch_page_theme": "^1.0",
        "drupal/token": "^1.9",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_link_attributes": "1.x-dev",
        "drupal/twig_tweak": "^2.9",
        "drupal/twig_vardumper": "2.2",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "^3.4",
        "drupal/variationcache": "^1.0",
        "drupal/verf": "^1.0",
        "drupal/views_bulk_operations": "^3.12",
        "drupal/views_templates": "^1.1",
        "drupal/viewsreference": "2.0-beta2",
        "drupal/webform": "^5.25",
        "drupal/wysiwyg_template": "^2.3",
        "drupal/xmlsitemap": "^1.0",
        "drupal/zurb_foundation": "~6.0",
        "drush/drush": "^10",
        "grasmash/drupal-security-warning": "^1.1",
        "pantheon-upstreams/upstream-configuration": "*",
        "vlucas/phpdotenv": "^2.4",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "conflict": {
            "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/composer/{$name}": ["type:drupal-module"],
            "web/profiles/composer/{$name}": ["type:drupal-profile"],
            "web/themes/composer/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    }
}
